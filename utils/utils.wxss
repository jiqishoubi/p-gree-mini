.flex{
  display: flex;
}
.flexCenter{
  display: flex;
  align-items: center;
}
.flexBetween{
  display: flex;
  justify-content: space-between;
}
.flexJCenter{
  display: flex;
  justify-content: center;
}
.flexAround{
  display: flex;
  justify-content: space-around;
}
.flexColumn{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.h100{
  height: 100%;
}
.img100{
  width: 100%;
  height: 100%;
}

/* 省略三个点 */

.textOmit {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.bg_white{
  background-color: #fff;
}